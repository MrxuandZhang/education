<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.edu.xu.mapper.NewsMapper">
    <cache/>
    <resultMap id="BaseResultMap" type="com.edu.xu.entity.News">
        <result column="uid" property="uid"></result>
        <result column="create_time" property="createTime"></result>
        <result column="content" property="content"></result>
        <result column="isNew" property="isNew"></result>
        <result column="state" property="state"></result>
        <result column="title" property="title"></result>
    </resultMap>

    <sql id="Base_Column_List">
    uid, account, dept_uid AS deptUid,password,phone,real_name AS realName, state,create_time AS createTime, update_time AS updateTime, is_del AS isDel, is_default AS isDefault
    </sql>

</mapper>
